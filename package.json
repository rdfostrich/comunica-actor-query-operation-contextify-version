{
  "name": "@comunica/actor-query-operation-contextify-version",
  "version": "1.0.0",
  "description": "A contextify-version query-operation actor",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@comunica/actor-query-operation-contextify-version",
  "lsd:components": "components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/contexts/comunica-actor-query-operation-contextify-version.jsonld": "components/context.jsonld"
  },
  "main": "index.js",
  "typings": "index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdfostrich/comunica-actor-query-operation-contextify-version.git"
  },
  "keywords": [
    "comunica",
    "actor",
    "query-operation",
    "contextify-version"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdfostrich/comunica-actor-query-operation-contextify-version/issues"
  },
  "homepage": "https://github.com/rdfostrich/comunica-actor-query-operation-contextify-version#readme",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "sparqlalgebrajs": "^0.7.4"
  },
  "peerDependencies": {
    "@comunica/core": "^1.0.0",
    "@comunica/bus-query-operation": "^1.0.0"
  },
  "devDependencies": {
    "@comunica/core": "^1.0.0",
    "@comunica/bus-query-operation": "^1.0.0",
    "@types/asynciterator": "^1.1.1",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.0.3",
    "@types/rdf-js": "^1.0.0",
    "arrayify-stream": "^1.0.0",
    "asynciterator": "^2.0.0",
    "coveralls": "^3.0.0",
    "jest": "^23.1.0",
    "pre-commit": "^1.2.2",
    "rdf-quad": "^1.1.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^2.5.3"
  },
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "coveralls-ci": "cat ./coverage/lcov.info | coveralls",
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "test-ci": "jest --ci --maxWorkers=4 --coverage",
    "lint": "tslint lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls"
  }
}
